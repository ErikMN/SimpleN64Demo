  "version": "0.2.0",
  "configurations": [
    {
      "name": "DEBUG",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/${workspaceFolderBasename}.out",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb-multiarch",
      "setupCommands": [
        {
          "description": "Setup MIPS arch",
          "text": "set arch mips:4300",
          "ignoreFailures": true
        },
        {
          "description": "Setup remote target",
          "text": "target remote localhost:8080",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
